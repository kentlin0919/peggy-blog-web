{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Next.js: Debug Client",
      "request": "launch",
      "type": "chrome",
      "url": "http://localhost:3000",
      "webRoot": "${workspaceFolder}"
    },
    {
      "name": "Attach to Supabase Functions",
      "type": "node",
      "request": "attach",
      "port": 9229,
      "address": "127.0.0.1",
      "localRoot": "${workspaceFolder}/supabase/functions",
      "remoteRoot": "/home/deno/functions",
      "restart": true,
      "sourceMaps": true
    }
  ]
}
